# MongoDB Atlas connection string
MONGO_URI=mongodb+srv://<username>:<password>@cluster0.mongodb.net/motorado?retryWrites=true&w=majority

# JWT secret key used to sign authentication tokens
JWT_SECRET=your_jwt_secret_here

# Amazon S3 configuration
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=your_aws_region
S3_BUCKET_NAME=your_s3_bucket_name

# The base URL of the frontend application (used for CORS)
CLIENT_URL=http://localhost:5173